
<% @artist_search_results.each do |song| %>

<%= song.name %>
<%= song.artist %>
<% unless song.artwork == '' %>
<img src="http://images.gs-cdn.net/static/albums/<%= song.artwork %>" />
<%end%>
<%= button_to 'Add', {action: 'create', song_id: song.id, song_name: song.name, song_artist: song.artist} %>

<%end%>
