<div class="playlist_find">
  <h1> Playlists nearby </h1>
  <% if @current_users_around.empty? %>
    <p class="welcome">No users currently. Please try again later</p>
  <% end %>

  <% @current_users_around.each do |u| %></br>
    <% @playlist = u.playlists.last %>
      <% if @playlist %>
        <% if u == @current_users_around[0] %>
          <div style='margin-left:90px'><%= u.username %></div>
        <%else%>
          <%= u.username %></br>
        <%end%>
        <div class="art_pic">
          <img class="searchart" src="http://static7.depositphotos.com/1001655/707/v/950/depositphotos_7070089-Woman-with-music-notes.jpg" />
          <%= link_to "#{@playlist.title}", playlist_path(@playlist.id), :class => 'playlists-around' %>
        </div>
      <% end %>
  <% end %>
</div>
